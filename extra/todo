typedef struct s_shell
{
	char **history;
	int n_cmds;
	char *location;
	t_cmd *cmds;
	t_var *var;
}	t_shell;

typedef struct s_cmd
{
	int fd[2]; (input and output)
	int append_flag;
	char *path_cmd; (includes exe path + cmd)
	char **cmd_args;
}	t_cmd;

typedef struct s_var
{
	char *name;
	void *value;
	t_var next;
}	t_var;